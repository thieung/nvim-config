{
    "name": "thieung-snippets",
    "engines": {
        "vscode": "^1.71.2"
    },
    "languages": [
        {
            "id": "all",
            "extensions": "*"
        }
    ],
    "contributes": {
        "snippets": [
            {
                "language": [
                    "all"
                ],
                "path": "./all.json"
            },
            {
                "language": "ruby",
                "path": "./ruby.json"
            },
            {
                "language": "rails",
                "path": "./rails.json"
            },
            {
                "language": "lua",
                "path": "./lua.json"
            },
            {
                "language": [
                    "markdown",
                    "rmd",
                    "rmarkdown"
                ],
                "path": "./markdown.json"
            }
        ]
    }
}
